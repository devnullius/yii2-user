{
    "name": "devnullius/yii2-user",
    "description": "User manage module for yii2",
    "type": "yii2-extension",
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4 < 8.0",
        "devnullius/yii2-helper": "^1.0",
        "devnullius/yii2-queue-addon": "^1.0",
        "filsh/yii2-oauth2-server": "^2.1",
        "kartik-v/yii2-field-range": "^1.3",
        "kartik-v/yii2-widget-datepicker": "^1.4",
        "la-haute-societe/yii2-save-relations-behavior": "^1.7",
        "ramsey/uuid": "^4.1",
        "webmozart/assert": "^1.9",
        "yiisoft/yii2": "^2.0",
        "yiisoft/yii2-authclient": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "aram",
            "email": "aram.ds@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true,
        "process-timeout": 1800
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "vcs",
            "url": "git@github.com:devnullius/yii2-helper.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "devnullius\\user\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "devnullius\\user\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    }
}
